<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_ejer1_b"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:orientation="vertical"
    tools:context="com.example.dm2.examen.Ejer1B">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Seleccion de Candidato"/>

    <TextView
        android:id="@+id/rsNombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nombre: "/>
    <TextView
        android:id="@+id/rsProvincia"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Provincia: "/>

    <TextView
        android:id="@+id/rsSexo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Sexo : "/>

    <TextView
        android:id="@+id/rsConocimientos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Conocimientos: "/>

    <Button
        android:id="@+id/bntAceptar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Aceptar"/>
    <Button
        android:id="@+id/btnRechazar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Rechazar"/>


</LinearLayout>
